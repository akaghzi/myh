<%- model_class = ContraceptiveHistory -%>
<div class="page-header">
	<h4>
		<span class="titlehighlight">
			<%= @contraceptive_history.patient.full_name.titleize %>
		</span>
		<span class="bluehighlight">
			<%= @contraceptive_history.patient.gender.titleize %>
		</span>
		<span class="titlehighlight">
			<%= @contraceptive_history.patient.age %>
		</span>
		<span class="bluehighlight">
			DOB:<%= @contraceptive_history.patient.date_of_birth %>
		</span>
	</h4>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:contraceptive_id) %>:</strong></dt>
  <dd><%= @contraceptive_history.contraceptive.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:contraceptive_problems) %>:</strong></dt>
  <dd><%= @contraceptive_history.contraceptive_problems %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              patient_path(id: @contraceptive_history.patient_id), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_contraceptive_history_path(@contraceptive_history), :class => 'btn' %>
  <!-- <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              contraceptive_history_path(@contraceptive_history),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %> -->
</div>
