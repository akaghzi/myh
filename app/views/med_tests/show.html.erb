<%- model_class = MedTest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:patient_id) %>:</strong></dt>
  <dd><%= @med_test.patient_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:visit_id) %>:</strong></dt>
  <dd><%= @med_test.visit_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:med_test_type_id) %>:</strong></dt>
  <dd><%= @med_test.med_test_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:measurement) %>:</strong></dt>
  <dd><%= @med_test.measurement %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mearsurement_note) %>:</strong></dt>
  <dd><%= @med_test.mearsurement_note %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ordered_at) %>:</strong></dt>
  <dd><%= @med_test.ordered_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reviewed_at) %>:</strong></dt>
  <dd><%= @med_test.reviewed_at %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              med_tests_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_med_test_path(@med_test), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              med_test_path(@med_test),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
