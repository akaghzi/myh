<span class="span4">
	<h4 class="alert">Lab Tests:&nbsp
	</h4>
	<% if @lab_tests.blank? %>
	<p>No lab test information found on record</p>
	<% else %>
	<ul>
		<% @visits.each do | visit | %>
		<% visit.lab_tests.each do | lab_test | %>
		<li>
			<%= link_to lab_test.full_name, edit_visit_lab_test_xref_path(id: lab_test.id), class: "link" %>
		</li>
		<% end %>
		<% end %>
	</ul>
	<% end %>
</span>
