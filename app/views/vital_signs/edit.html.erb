<%- model_class = VitalSign -%>
<div class="page-header">
  <h4>
		<span class="titlehighlight">
			<%= @vital_sign.visit.patient.full_name.titleize %>
		</span>
		<span class="bluehighlight">
		<%= @vital_sign.visit.patient.gender.titleize %>
	</span>
		<span class="titlehighlight">
			<%= @vital_sign.visit.patient.age %>
		</span>
		<span class="bluehighlight">
			DOB: <%= @vital_sign.visit.patient.date_of_birth %>
		</span>
	</h4>
</div>

<%= render :partial => 'form' %>
