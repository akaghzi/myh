<span class="span6">
	<h4 class="alert alert-info">
		Contraceptive History:&nbsp
		<%= link_to 'Add Record', new_contraceptive_history_path(patient_id: @patient.id), class: "btn btn-small btn-primary" %>
	</h4>
	<% if @contraceptive_histories.blank? %>
	<p>
		No contraceptive history information found on record
	</p>
	<% else %>
	<table class="table table-condensed">
		<tr>
			<th>Contraceptive Type</th>
			<th>Contraceptive Problems</th> 
			<td></td>
		</tr>
	
		<% @contraceptive_histories.each do | contraceptive_history | %>
		<tr>
			<td><%= contraceptive_history.contraceptive.name %></td>
			<td><%= contraceptive_history.contraceptive_problems %></td>
			<td><%= link_to 'Edit', edit_contraceptive_history_path(contraceptive_history), class: "btn btn-mini btn-info"%></td>
		</tr>
		<% end %>
	</table>
	<% end %>
</span>