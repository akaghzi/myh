<%= form_for @visit_lab_test_xref, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :visit_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :visit_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :lab_test_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lab_test_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :measurement, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :measurement, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :received_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :received_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :reviewed_at, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :reviewed_at, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :lab_note, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :lab_note, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                visit_lab_test_xrefs_path, :class => 'btn' %>
  </div>
<% end %>
