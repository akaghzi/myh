<%- model_class = InsuranceInfo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:patient_id) %></th>
      <th><%= model_class.human_attribute_name(:insurance_carrier) %></th>
      <th><%= model_class.human_attribute_name(:insurance_member_id) %></th>
      <th><%= model_class.human_attribute_name(:insurance_guarantor) %></th>
      <th><%= model_class.human_attribute_name(:insurance_guarantor_relationship) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @insurance_infos.each do |insurance_info| %>
      <tr>
        <td><%= link_to insurance_info.id, insurance_info_path(insurance_info) %></td>
        <td><%= insurance_info.patient_id %></td>
        <td><%= insurance_info.insurance_carrier %></td>
        <td><%= insurance_info.insurance_member_id %></td>
        <td><%= insurance_info.insurance_guarantor %></td>
        <td><%= insurance_info.insurance_guarantor_relationship %></td>
        <td><%=l insurance_info.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_insurance_info_path(insurance_info), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      insurance_info_path(insurance_info),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_insurance_info_path,
            :class => 'btn btn-primary' %>
