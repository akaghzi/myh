<%- model_class = GynecologyHistory -%>
<div class="page-header">
	<h4>
		<span class="titlehighlight">
			<%= @gynecology_history.patient.full_name.titleize %>
		</span>
		<span class="bluehighlight">
			<%= @gynecology_history.patient.gender.titleize %>
		</span>
		<span class="titlehighlight">
			<%= @gynecology_history.patient.age %>
		</span>
		<span class="bluehighlight">
			DOB:<%= @gynecology_history.patient.date_of_birth %>
		</span>
	</h4>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:problem_id) %>:</strong></dt>
  <dd><%= @gynecology_history.gynecology_problem.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:problem_date) %>:</strong></dt>
  <dd><%= @gynecology_history.problem_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:followup) %>:</strong></dt>
  <dd><%= @gynecology_history.followup %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              patient_path(id: @gynecology_history.patient_id), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_gynecology_history_path(@gynecology_history), :class => 'btn' %>
  <!-- <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              gynecology_history_path(@gynecology_history),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %> -->
</div>
