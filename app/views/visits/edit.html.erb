<%- model_class = Visit -%>
<h4>Update <%= model_class %></h4>
<div class="page-header">
  <h4>
		<span class="titlehighlight">
			<%= @visit.patient.full_name.titleize %>
		</span>
		<span class="bluehighlight">
		<%= @visit.patient.gender.titleize %>
	</span>
		<span class="titlehighlight">
			<%= @visit.patient.age %>
		</span>
		<span class="bluehighlight">
			DOB: <%= @visit.patient.date_of_birth %>
		</span>
		
	</h4>
</div>
<%= render :partial => 'form' %>
