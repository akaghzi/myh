<%- model_class = InsuranceInfo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:patient_id) %>:</strong></dt>
  <dd><%= @insurance_info.patient_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:insurance_carrier) %>:</strong></dt>
  <dd><%= @insurance_info.insurance_carrier %></dd>
  <dt><strong><%= model_class.human_attribute_name(:insurance_member_id) %>:</strong></dt>
  <dd><%= @insurance_info.insurance_member_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:insurance_guarantor) %>:</strong></dt>
  <dd><%= @insurance_info.insurance_guarantor %></dd>
  <dt><strong><%= model_class.human_attribute_name(:insurance_guarantor_relationship) %>:</strong></dt>
  <dd><%= @insurance_info.insurance_guarantor_relationship %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              insurance_infos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_insurance_info_path(@insurance_info), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              insurance_info_path(@insurance_info),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
