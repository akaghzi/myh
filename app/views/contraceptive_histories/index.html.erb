<%- model_class = ContraceptiveHistory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:patient_id) %></th>
      <th><%= model_class.human_attribute_name(:contraceptive_id) %></th>
      <th><%= model_class.human_attribute_name(:contraceptive_problems) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @contraceptive_histories.each do |contraceptive_history| %>
      <tr>
        <td><%= link_to contraceptive_history.id, contraceptive_history_path(contraceptive_history) %></td>
        <td><%= contraceptive_history.patient_id %></td>
        <td><%= contraceptive_history.contraceptive.name %></td>
        <td><%= contraceptive_history.contraceptive_problems %></td>
        <td><%=l contraceptive_history.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_contraceptive_history_path(contraceptive_history), :class => 'btn btn-mini' %>
          <!-- <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      contraceptive_history_path(contraceptive_history),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %> -->
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_contraceptive_history_path,
            :class => 'btn btn-primary' %>
