<%- model_class = Immunization -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:patient_id) %>:</strong></dt>
  <dd><%= @immunization.patient.full_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:immunization_for) %>:</strong></dt>
  <dd><%= @immunization.immunization_for %></dd>
  <dt><strong><%= model_class.human_attribute_name(:immunization_year) %>:</strong></dt>
  <dd><%= @immunization.immunization_year %></dd>
  </dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              patient_path(id: @immunization.patient.id), :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_immunization_path(@immunization), :class => 'btn btn-info' %>
<!--  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              immunization_path(@immunization),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %> -->
</div>
