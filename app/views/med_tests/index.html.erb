<%- model_class = MedTest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:patient_id) %></th>
      <th><%= model_class.human_attribute_name(:visit_id) %></th>
      <th><%= model_class.human_attribute_name(:med_test_type_id) %></th>
      <th><%= model_class.human_attribute_name(:measurement) %></th>
      <th><%= model_class.human_attribute_name(:mearsurement_note) %></th>
      <th><%= model_class.human_attribute_name(:ordered_at) %></th>
      <th><%= model_class.human_attribute_name(:reviewed_at) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @med_tests.each do |med_test| %>
      <tr>
        <td><%= link_to med_test.id, med_test_path(med_test) %></td>
        <td><%= med_test.patient_id %></td>
        <td><%= med_test.visit_id %></td>
        <td><%= med_test.med_test_type_id %></td>
        <td><%= med_test.measurement %></td>
        <td><%= med_test.mearsurement_note %></td>
        <td><%= med_test.ordered_at %></td>
        <td><%= med_test.reviewed_at %></td>
        <td><%=l med_test.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_med_test_path(med_test), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      med_test_path(med_test),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_med_test_path,
            :class => 'btn btn-primary' %>
