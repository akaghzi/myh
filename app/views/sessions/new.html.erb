<h1>Log In</h1>
<div class="control-group">
	<div class="controls">
		<%= form_for(:session, url: sessions_path) do|f|%>
			<%= f.label :username %>
			<%= f.text_field :username, class: "loginname" %>
			<%= f.label :password %>
			<%= f.password_field :password %><br />
			<%= f.submit "Login", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>