<%- model_class = Visit -%>
<div class="page-header">
  <h1>
		<span class="titlehighlight">
			<%= @visit.patient.full_name.titleize %>
		</span>
		<span class="genderhighlight">
		<%= @visit.patient.gender.titleize %>
	</span>
		<span class="titlehighlight">
			<%= @visit.patient.age %>
		</span>
	</h1>
</div>
<%= render :partial => 'form' %>
